<%=javascript_pack_tag 'tickets'%>
<%=form_for @ticket do |f|%><br>
  Ticket Name :- <%=f.text_field :name, placeholder: "Name"%><br><br>
  Project Name :- <%=f.select :project_name, options_for_select([['Bsnl-wifi-testing','Bsnl-wifi-testing'],['Bsnl-uso-onm-int', 'Bsnl-uso-onm-int'],['Bsnl-uso-o&m', 'Bsnl-uso-o&m']])%><br><br>
  State :- <%=f.collection_select :state_id, State.all, :id, :name%><br><br>
  District :- <%=f.collection_select :district_id, District.all, :id, :name%><br><br>
  Location :- <%=f.text_field :location, placeholder: "Location"%><br><br>
  Ticket Status :- <%=f.select :status, options_for_select([['Open', 'open'], ['In Progress', 'in_progress'],['Close', 'close']])%><br><br>
  <%=f.submit class: "btn btn-primary"%>
  <%=link_to "Back", tickets_path%>
<% end -%>