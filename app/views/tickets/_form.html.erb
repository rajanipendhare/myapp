<%=form_for @ticket do |f|%>
  <%=f.text_field :name, placeholder: "name"%><br><br>
  <%=f.text_field :description, placeholder: "description"%><br><br>
  <%=f.select :status, options_for_select([['Open', 'open'], ['In Progress', 'in_progress'],['Close', 'close']])%><br><br>
  <%=f.text_field :location, placeholder: "location"%><br><br>
  <%=f.text_field :project_name, placeholder: "project_name"%><br><br>
  <%=f.submit class: "btn btn-primary"%>
  <%=link_to "Back", root_path%>
<% end -%>